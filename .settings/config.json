{
    "version": "0.0.1",
    "main": "./dist/typescript-modular-boilerplate.js",
    "sourcePath": "src",
    "customBuild": {
        "command": "jake --jakefile Jakefile.js"
    },
    "builds": [
        {
            "name": "typescriptModularBoilerplate",
            "main": "",
            "output": "dist/typescript-modular-boilerplate.js"
        }
    ],
    "compiler": {
        "useDefaultLib": true,
        "emitComments": false,
        "outputOption": "/home/david/dev/geometryzen/typescript-modular-boilerplate/tmp",
        "generateDeclarationFiles": false,
        "mapSourceFiles": false,
        "codeGenTarget": 1,
        "moduleGenTarget": 0
    },
    "completionMode": "loose",
    "minify": false,
    "rememberOpenFiles": true
}